{
  "name": "vinyl",
  "version": "0.4.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run lint && webpack --progress --colors --display-error-details --config webpack/prod.config.js",
    "build:components": "webpack --progress --colors --display-error-details --config src/components/webpack.prod.config.js",
    "build:scrollsync:module": "webpack --progress --colors --display-error-details --config src/modules/ScrollSync/webpack.prod.config.js",
    "build:suggest:module": "webpack --progress --colors --display-error-details --config src/modules/Suggest/webpack.prod.config.js",
    "build:select:module": "webpack --progress --colors --display-error-details --config src/modules/Select/webpack.prod.config.js",
    "build:suggest:app": "rimraf examples/suggestApp && webpack --progress --colors --display-error-details --config src/modules/SuggestApp/webpack.client.config.js",
    "build:suggest:app:server": "webpack --progress --colors --display-error-details --config src/modules/SuggestApp/webpack.server.config.js",
    "check": "flow check",
    "clean": "rimraf dist",
    "explore:components": "source-map-explorer dist/vinyl-components.min.js dist/vinyl-components.min.map",
    "explore:scrollsync:module": "source-map-explorer dist/vinyl-scrollsync-module.min.js dist/vinyl-scrollsync-module.min.map",
    "explore:suggest:module": "source-map-explorer dist/vinyl-suggest-module.min.js dist/vinyl-suggest-module.min.map",
    "lint": "eslint src",
    "start": "webpack-dev-server --colors --inline --hot --no-info --config webpack/dev.config.js --host 0.0.0.0 --port 8000",
    "start:suggest:app": "webpack-dev-server --colors --inline --hot --no-info --config webpack/dev.suggestApp.config.js --host 0.0.0.0 --port 8000",
    "test": "npm run check && npm run build && npm run testonly",
    "testonly": "mocha --require resources/mocha-bootload src/**/__tests__/**/*.js",
    "watch:mongo-service": "./node_modules/.bin/nodemon --exec c:/node7/node --harmony-async-await services/mongodb",
    "watch": "npm-run-all watch:*"
  },
  "author": "Henry Tao",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.28.4",
    "eslint": "^3.13.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-import": "^2.2.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.37.4",
    "html-webpack-plugin": "^2.26.0",
    "micro": "^7.3.3",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.5.4",
    "rx-web-js": "github:once-ler/rx-web-js#master",
    "should": "^11.1.2",
    "style-loader": "^0.18.1",
    "tingodb": "^0.5.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "autosuggest-highlight": "^3.1.0",
    "axios": "^0.16.2",
    "easy-react-form": "^1.0.6",
    "history": "^4.5.0",
    "ie-version": "^0.1.0",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.22.2",
    "react": "^16.5.2",
    "react-art": "^16.5.2",
    "react-autosuggest": "^9.0.1",
    "react-big-calendar": "^0.20.1",
    "react-collapse": "^4.0.2",
    "react-dom": "^16.5.2",
    "react-dropzone": "^3.13.1",
    "react-motion": "^0.5.0",
    "react-native": "^0.57.2",
    "react-native-responsive-grid": "^0.41.992",
    "react-native-web": "^0.9.1",
    "react-portal-minimal": "^4.0.2",
    "react-progress-2": "^4.4.1",
    "react-redux": "5.0.7",
    "react-router": "3.0.4",
    "react-router-redux": "^4.0.7",
    "react-view-pager": "^0.5.1",
    "react-virtualized": "^9.7.0",
    "react-virtualized-select": "^3.0.1",
    "recompose": "^0.23.5",
    "redux": "^3.6.0",
    "redux-implicit-oauth2": "^1.0.1",
    "styled-components": "3.4.10",
    "tcomb-form-native": "^0.6.19"
  }
}
